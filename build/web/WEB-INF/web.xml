<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddModuleController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.AddModuleController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddTeacherController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.AddTeacherController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddClassroomController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.AddClassroomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddGroupController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.AddGroupController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyClassroomController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.ModifyClassroomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyGroupController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.ModifyGroupController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyTeacherController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.ModifyTeacherController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyModuleController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.ModifyModuleController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TeacherModuleController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.TeacherModuleController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteTeacherModuleController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.DeleteTeacherModuleController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteGroupModuleController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.DeleteGroupModuleController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GroupModuleController</servlet-name>
        <servlet-class>com.nikesh.scheduler.controller.GroupModuleController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutController</url-pattern>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddModuleController</servlet-name>
        <url-pattern>/AddModuleController</url-pattern>
        <url-pattern>/addmodule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddTeacherController</servlet-name>
        <url-pattern>/AddTeacherController</url-pattern>
        <url-pattern>/addteacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddClassroomController</servlet-name>
        <url-pattern>/AddClassroomController</url-pattern>
        <url-pattern>/addclassroom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddGroupController</servlet-name>
        <url-pattern>/AddGroupController</url-pattern>
        <url-pattern>/addgroup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifyClassroomController</servlet-name>
        <url-pattern>/ModifyClassroomController</url-pattern>
        <url-pattern>/modifyclassroom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifyGroupController</servlet-name>
        <url-pattern>/ModifyGroupController</url-pattern>
        <url-pattern>/modifygroup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifyTeacherController</servlet-name>
        <url-pattern>/ModifyTeacherController</url-pattern>
        <url-pattern>/modifyteacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifyModuleController</servlet-name>
        <url-pattern>/ModifyModuleController</url-pattern>
        <url-pattern>/modifymodule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TeacherModuleController</servlet-name>
        <url-pattern>/TeacherModuleController</url-pattern>
        <url-pattern>/teachermodule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteTeacherModuleController</servlet-name>
        <url-pattern>/DeleteTeacherModuleController</url-pattern>
        <url-pattern>/DeleteTeacherModule</url-pattern>
        <url-pattern>/deleteteachermodule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteGroupModuleController</servlet-name>
        <url-pattern>/DeleteGroupModuleController</url-pattern>
        <url-pattern>/deletegroupmodule</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GroupModuleController</servlet-name>
        <url-pattern>/GroupModuleController</url-pattern>
        <url-pattern>/groupmodule</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
